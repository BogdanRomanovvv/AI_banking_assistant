# Environment variables for Docker Compose
# Copy this file to .env and fill in your actual values

# Yandex GPT Configuration
YANDEX_API_KEY=your_api_key_here
YANDEX_FOLDER_ID=your_folder_id_here
YANDEX_MODEL=yandexgpt

# Yandex Mail IMAP Configuration
# Для получения пароля приложения см. MAIL_SETUP.md
YANDEX_MAIL_LOGIN=your_email@yandex.ru
YANDEX_MAIL_PASSWORD=your_app_password_here
YANDEX_MAIL_IMAP_SERVER=imap.yandex.ru
YANDEX_MAIL_IMAP_PORT=993
YANDEX_MAIL_CHECK_INTERVAL=60
